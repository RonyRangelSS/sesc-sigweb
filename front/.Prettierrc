{
    "$schema": "https://json.schemastore.org/prettierrc",
    "plugins": ["prettier-plugin-tailwindcss"],
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "useTabs": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "parser": "typescript",
    "singleAttributePerLine": true,
    "quoteProps": "as-needed",
    "embeddedLanguageFormatting": "auto"
}

