{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titulo": {
      "type": "string"
    },
    "imagem": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "categoria": {
      "type": "string"
    },
    "descricao": {
      "type": "text"
    },
    "endereco": {
      "type": "string"
    },
    "local": {
      "type": "customField",
      "options": {
        "info": true
      },
      "customField": "plugin::geodata.geojson"
    },
    "contato": {
      "type": "string"
    },
    "horario": {
      "type": "time"
    }
  }
}
